<!DOCTYPE html>
<html>
<head lang="en">
{{#if Nonce}}
	<meta http-equiv="Content-Security-Policy" content="default-src 'none'; script-src 'nonce-{{Nonce}}'; style-src 'nonce-{{Nonce}}'; connect-src 'self'; img-src 'self'; font-src 'self';"></span>
{{/if}}
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
	<link{{NonceAttribute}} href="/libs/jquery-ui.structure.css" rel="stylesheet" type="text/css"></span>
	<link{{NonceAttribute}} href="/libs/jquery-ui.min.css" rel="stylesheet" type="text/css"></span>
	<link{{NonceAttribute}} href="/libs/theme.min.css" rel="stylesheet" type="text/css"></span>
	<link{{NonceAttribute}} href="/libs/jquery.multiselect.css" rel="stylesheet" type="text/css"></span>
	<link{{NonceAttribute}} href="/libs/jquery.dataTables.min.css" rel="stylesheet" type="text/css"></span>
	<script{{NonceAttribute}} src="/libs/jquery.min.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/underscore-min.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/jquery-ui.min.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/jquery.dataTables.min.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/jquery.multiselect.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/bowser.min.js" type="text/javascript"></script>
	<script{{NonceAttribute}} src="/libs/murmurhash.js" type="text/javascript"></script>
	<link{{NonceAttribute}} id="defaultStyles" href="/default{{VersionSuffix}}.css" rel="stylesheet" type="text/css"></span>
	<link{{NonceAttribute}} href="/skin/{{Settings.Skin}}{{VersionSuffix}}.css" rel="stylesheet" type="text/css"></span>
	<script{{NonceAttribute}} src="/default{{VersionSuffix}}.js" type="text/javascript"></script>
	{{include default.inc}}
	{{HeaderScript}}
	<script{{NonceAttribute}} src="/skin/{{Settings.Skin}}{{VersionSuffix}}.js" type="text/javascript"></script>
	<title>{{Config.Filename}} - {{Server.Namespace}} - {{{Title}}}</title>
	{{Head}}
</head>
<body>
<img id="menuicon" src="/images/menu.png"></span>
<div id="outerheader">
<div id="header">
{{#if Session.User}}<div class="loginuser">{{Session.User.Login}}</div>{{/if}}
<div id="heading">{{Config.Filename}} - {{{Server.Namespace}}} - {{{Title}}}</div>
<div id="menu1">&nbsp;{{#each Modules}}
	<button id="{{Name}}" href="/{{Name}}/default.html">{{UnCamelName}}</button>{{/each}}
	<button target="help" href="/help/default.md">Help Contents</button>
	{{#if Help}}<button target="help" href="{{Help}}">Help</button>{{/if}}
</div>
<div id="menu2">&nbsp;{{#each Menu}}<button id="{{Id}}" href="{{Url}}"{{#if Disabled}} disabled="disabled"{{/if}}>{{Text}}</button>{{/each}}</div>
</div>
<div id="menu3"><span class="back-pos"></span><span class="download-pos"></span><span class="openDialog-pos"></span><span class="save-pos"></span><span class="apply-pos"></span><span class="savenew-pos"></span><span class="delete-pos"></span><span class="reset-pos"></span><span class="next-pos"></span><span class="previous-pos"></span><span class="download-pos"></span><span class="other-pos"></span>&nbsp;</div>
</div>
<div id="spacer"></div>
<div id="body">
<div id="message">{{Message}}&nbsp;</div>
{{Body}}
</div>
</body>
</html>